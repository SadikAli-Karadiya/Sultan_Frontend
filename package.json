{
  "name": "Sultan Mobile",
  "version": "0.1.0",
  "main": "public/main.js",
  "private": true,
  "homepage": "./",
  "author": "Wellbenix",
  "description": "Fees management software",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-build": "electron-builder -mwl",
    "electron:build": "electron-builder -c.extraMetadata.main=client/main.js",
    "electron-react": "concurrently \"cross-env npm start\" \"wait-on http://localhost:3000\""
  },
  "build": {
    "extends": null,
    "appId": "com.wellbenix.nasir_sir_classes",
    "productName": "Nasir_Sir_And_Classes",
    "copyright": "Copyright Â© 2022 Wellbenix",
    "files": [
      "dist/**/*",
      "build/**/*",
      "node_modules/**/*",
      "package.json",
      "public/**/*"
    ],
    "win": {
      "icon": "public/software_icon.png"
    },
    "mac": {
      "icon": "public/software_icon.png"
    },
    "directories": {
      "buildResources": "client/public"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@headlessui/react": "^1.7.14",
    "@material-tailwind/react": "^2.0.0",
    "@mui/material": "^5.13.0",
    "@tippyjs/react": "^4.2.6",
    "axios": "^1.4.0",
    "export-from-json": "^1.7.3",
    "formik": "^2.2.9",
    "moment": "^2.29.4",
    "qs": "^6.11.2",
    "react": "^18.2.0",
    "react-apexcharts": "^1.4.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-icons": "^3.11.0",
    "react-pro-sidebar": "^1.0.0",
    "react-query": "^3.39.3",
    "react-redux": "^8.0.5",
    "react-responsive-pagination": "^2.1.0",
    "react-router-dom": "^6.11.1",
    "react-scripts": "^5.0.1",
    "react-select": "^5.7.3",
    "react-to-print": "^2.14.13",
    "react-toastify": "^9.1.2",
    "sweetalert2": "^11.7.5",
    "tailwindcss-debug-screens": "^2.2.1",
    "yup": "^0.32.11",
    "yup-phone": "^1.3.2"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^3.0.0",
    "autoprefixer": "^10.4.13",
    "electron": "^25.3.0",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.4",
    "vite": "^4.0.0"
  }
}
